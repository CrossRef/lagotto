<dt>Status</dt>
<dd><%= state_label(@source.human_state_name).html_safe %></dd>
<% unless @source.group.blank? %>
  <dt>Group</dt>
  <dd><%= h(@source.group.display_name) %></dd>
<% end %>
<% unless @source.description.blank? %>
  <dt>Description</dt>
  <dd><%= h(@source.description) %></dd>
<% end %>
